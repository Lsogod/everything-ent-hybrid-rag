APP_NAME=Everything-Ent-Hybrid
APP_VERSION=1.1.0
APP_ENV=dev
API_PREFIX=/api/v1
LOG_LEVEL=INFO
PYTHON_IMAGE=python:3.10-slim
PIP_INDEX_URL=https://pypi.tuna.tsinghua.edu.cn/simple
PIP_TRUSTED_HOST=pypi.tuna.tsinghua.edu.cn
ES_IMAGE=swr.cn-north-4.myhuaweicloud.com/ddn-k8s/docker.io/library/elasticsearch:8.11.3

# Optional API auth for all endpoints
API_KEY=

# Qwen3-Embedding via OpenAI SDK compatible base_url (backend only)
EMBEDDING_MODE=cloud
EMBEDDING_DIM=1024
EMBEDDING_MODEL_CLOUD=text-embedding-v4
EMBEDDING_MODEL_LOCAL=gte-Qwen2-1.5B-instruct
EMBEDDING_BASE_URL_CLOUD=https://dashscope.aliyuncs.com/compatible-mode/v1
EMBEDDING_BASE_URL_LOCAL=http://host.docker.internal:8001/v1
EMBEDDING_USE_DIMENSIONS=true
EMBEDDING_BATCH_SIZE=10
# DASHSCOPE_API_KEY=sk-xxxx
DASHSCOPE_API_KEY=
LOCAL_EMBEDDING_API_KEY=

# Qwen LLM via OpenAI SDK compatible base_url (backend only)
LLM_PROVIDER=qwen
LLM_MODEL=qwen-plus
LLM_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
# LLM_API_KEY=sk-xxxx
LLM_API_KEY=

# Elasticsearch
ES_URL=http://elasticsearch:9200
ES_INDEX=ent_kb_chunks
ES_VERIFY_CERTS=false
ES_NUM_CANDIDATES=100

# Queue
REDIS_URL=redis://redis:6379/0
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0
DATABASE_URL=sqlite:////tmp/ent_hybrid.db
ACL_ENABLED=true
ACL_BOOTSTRAP=true
ACL_DEFAULT_ADMIN_USER=admin

# Retrieval
RETRIEVAL_TOP_K_KEYWORD=10
RETRIEVAL_TOP_K_VECTOR=10
RETRIEVAL_TOP_K_FINAL=5
RRF_K=60

# ETL and watcher
CHUNK_SIZE=500
CHUNK_OVERLAP=50
MAX_FILE_SIZE_MB=100
WATCH_EXTENSIONS=.pdf,.docx,.md,.txt,.py,.java,.js,.ts,.go,.sql
KNOWLEDGE_ROOT=/data/knowledge
ALLOW_EXTERNAL_PATHS=false
API_PORT=18000
FRONTEND_PORT=5173
FRONTEND_NODE_IMAGE=swr.cn-north-4.myhuaweicloud.com/ddn-k8s/docker.io/library/node:22-alpine
NPM_REGISTRY=https://registry.npmmirror.com
